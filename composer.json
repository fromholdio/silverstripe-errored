{
  "name": "fromholdio/silverstripe-errored",
  "type": "silverstripe-vendormodule",
  "description": "Alternate handling of displaying http errors",
  "license": "BSD-3-Clause",
  "keywords": ["silverstripe", "errors", "dev"],
  "homepage": "https://github.com/fromholdio/silverstripe-errored",
  "support": {
    "issues": "https://github.com/fromholdio/silverstripe-errored/issues"
  },
  "authors": [{
    "name": "Luke Fromhold",
    "homepage": "https://fromhold.io"
  }],
  "require": {
    "silverstripe/cms": "~4.10 || ~5.0",
    "symfony/filesystem": "^6.1"
  },
  "replace": {
    "silverstripe/errorpage":"*"
  },
  "autoload": {
    "psr-4": {
      "Fromholdio\\Errored\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
